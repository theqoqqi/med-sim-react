{
  "title": "Все параметры",
  "type": "composite",
  "parameters": {
    "physical": {
      "title": "Физические показатели",
      "type": "composite",
      "parameters": {
        "age": {
          "title": "Возраст",
          "type": "number",
          "randomType": "gaussian",
          "randomOptions": {
            "meanOrigin": 0.35,
            "deviationFactor": 0.5
          },
          "normalRange": [0, 70],
          "viableRange": [0, 1000]
        },
        "gender": {
          "title": "Пол",
          "type": "enum",
          "allowedValues": ["male", "female"]
        },
        "height": {
          "title": "Рост",
          "type": "number",
          "randomType": "gaussian",
          "normalRange": [120, 220],
          "viableRange": [50, 300]
        },
        "weight": {
          "title": "Вес",
          "type": "number",
          "randomType": "gaussian",
          "fluctuation": "low",
          "normalRange": [40, 120],
          "viableRange": [20, 200]
        }
      }
    },
    "physiological": {
      "title": "Физиологические показатели",
